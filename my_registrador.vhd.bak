-- A library clause declares a name as a library.  It 
-- does not create the library; it simply forward declares 
-- it. 
library ieee;

-- Use clauses import declarations into the current scope.	
-- If more than one use clause imports the same name into the
-- the same scope, none of the names are imported.

-- Import all the declarations in a package
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;




entity my_registrador is
	Generic ( 
		DATA_SIZE : natural := 1
	);
	
	port
	(
		entrada : in std_logic_vector(DATA_SIZE-1 downto 0);
		clk : in std_logic;
		
		saida : out std_logic_vector(DATA_SIZE-1 downto 0)
	);
end my_registrador;


-- Library Clause(s) (optional)
-- Use Clause(s) (optional)
architecture regArch of my_registrador is

	signal sig_key : std_logic_vector(DATA_SIZE-1 downto 0);
	
begin
	
	--sig_key <= ( std_logic_vector(to_unsigned(0, key_out'length - key_in'length)) & key_in);
	sig_key <= ( "000000" & not(key_in));
	
	key_out <= sig_key when (enable = '1') else (others=>'Z');
	
end regArch;



